{
  "dashboard": {
    "id": null,
    "uid": "healthcare-observability",
    "title": "Healthcare Observability Dashboard",
    "tags": ["healthcare", "observability"],
    "timezone": "browser",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 38,
    "version": 0,
    "templating": {
      "list": [
        {
          "name": "service_name",
          "type": "query",
          "label": "Service Name",
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "query": "SELECT DISTINCT ServiceName as __text, ServiceName as __value FROM observability.otel_traces ORDER BY ServiceName",
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "includeAll": true,
          "allValue": null,
          "refresh": 1
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Token Usage (Total)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "datasource": {
              "type": "grafana-clickhouse-datasource",
              "uid": "clickhouse"
            },
            "rawSql": "SELECT SUM(toInt64OrZero(SpanAttributes['llm.usage.total_tokens'])) as total_tokens FROM observability.otel_traces WHERE $__timeFilter(Timestamp) AND ($service_name = '' OR ServiceName = '$service_name') AND SpanAttributes['llm.usage.total_tokens'] != ''",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10000},
                {"color": "orange", "value": 100000},
                {"color": "red", "value": 1000000}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 2,
        "title": "Token Usage Over Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0},
        "targets": [
          {
            "datasource": {
              "type": "grafana-clickhouse-datasource",
              "uid": "clickhouse"
            },
            "rawSql": "SELECT toStartOfMinute(Timestamp) as time, SUM(toInt64OrZero(SpanAttributes['llm.usage.total_tokens'])) as total_tokens FROM observability.otel_traces WHERE $__timeFilter(Timestamp) AND ($service_name = '' OR ServiceName = '$service_name') AND SpanAttributes['llm.usage.total_tokens'] != '' GROUP BY time ORDER BY time",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Agent Error Count",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "datasource": {
              "type": "grafana-clickhouse-datasource",
              "uid": "clickhouse"
            },
            "rawSql": "SELECT COUNT(*) as error_count FROM observability.otel_traces WHERE $__timeFilter(Timestamp) AND ($service_name = '' OR ServiceName = '$service_name') AND SpanKind = 'SPAN_KIND_SERVER' AND (StatusCode = 'STATUS_CODE_ERROR' OR SpanAttributes['error'] = 'true' OR SpanAttributes['error.type'] != '')",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "orange", "value": 5},
                {"color": "red", "value": 10}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "Agent Errors Over Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "datasource": {
              "type": "grafana-clickhouse-datasource",
              "uid": "clickhouse"
            },
            "rawSql": "SELECT toStartOfMinute(Timestamp) as time, COUNT(*) as error_count FROM observability.otel_traces WHERE $__timeFilter(Timestamp) AND ($service_name = '' OR ServiceName = '$service_name') AND SpanKind = 'SPAN_KIND_SERVER' AND (StatusCode = 'STATUS_CODE_ERROR' OR SpanAttributes['error'] = 'true' OR SpanAttributes['error.type'] != '') GROUP BY time ORDER BY time",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Token Usage by Agent",
        "type": "barchart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "datasource": {
              "type": "grafana-clickhouse-datasource",
              "uid": "clickhouse"
            },
            "rawSql": "SELECT SpanAttributes['agent.name'] as agent_name, SUM(toInt64OrZero(SpanAttributes['llm.usage.total_tokens'])) as total_tokens FROM observability.otel_traces WHERE $__timeFilter(Timestamp) AND ($service_name = '' OR ServiceName = '$service_name') AND SpanAttributes['llm.usage.total_tokens'] != '' AND SpanAttributes['agent.name'] != '' GROUP BY agent_name ORDER BY total_tokens DESC LIMIT 10",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      }
    ]
  }
}

