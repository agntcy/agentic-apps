            "uid": "clickhouse"
          },
          "rawSql": "SELECT COUNT(*) as error_count FROM observability.otel_traces WHERE $__timeFilter(Timestamp)  AND SpanKind = 'SPAN_KIND_SERVER' AND (StatusCode = 'STATUS_CODE_ERROR' OR SpanAttributes['error'] = 'true' OR SpanAttributes['error.type'] != '')",
          "format": 1,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
