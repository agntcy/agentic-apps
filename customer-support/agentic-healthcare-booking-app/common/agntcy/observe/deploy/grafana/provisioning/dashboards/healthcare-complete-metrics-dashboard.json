{
  "uid": "healthcare-complete-metrics",
  "title": "Healthcare Complete Metrics Dashboard",
  "tags": [
    "healthcare",
    "mce",
    "all-metrics"
  ],
  "timezone": "browser",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "refresh": "30s",
  "schemaVersion": 38,
  "version": 12,
  "templating": {
    "list": [
      {
        "name": "service_name",
        "type": "query",
        "label": "Service Name",
        "datasource": {
          "type": "grafana-clickhouse-datasource",
          "uid": "clickhouse"
        },
        "query": "SELECT DISTINCT ServiceName as __text, ServiceName as __value FROM observability.otel_traces ORDER BY ServiceName",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "includeAll": true,
        "allValue": "",
        "refresh": 1,
        "multi": false,
        "hide": 2
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Token Usage (Total)",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "clickhouse"
          },
          "rawSql": "SELECT SUM(toInt64OrZero(SpanAttributes['llm.usage.total_tokens'])) as total_tokens FROM observability.otel_traces WHERE $__timeFilter(Timestamp)  AND SpanAttributes['llm.usage.total_tokens'] != ''",
          "format": 1,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10000
              },
              {
                "color": "orange",
                "value": 100000
              },
              {
                "color": "red",
                "value": 1000000
              }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
